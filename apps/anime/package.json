    "@graphql-typed-document-node/core": "3.2.0",
    "@joulev/theme": "workspace:*",
    "@sveltejs/adapter-vercel": "2.4.2",
    "@sveltejs/kit": "1.15.7",
    "@types/node": "18.16.0",
    "@types/nprogress": "0.2.0",
