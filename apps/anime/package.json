{
  "name": "@joulev/app-anime",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "graphql-codegen --config codegen.yml --watch & vite dev --port 3003",
    "build": "vite build",
    "preview": "vite preview --port 3003",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "postinstall": "svelte-kit sync && graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.7.0",
    "graphql": "16.6.0",
    "nprogress": "0.2.0",
    "svelte-material-icons": "2.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.5",
    "@graphql-codegen/client-preset": "1.0.5",
    "@graphql-typed-document-node/core": "3.1.1",
    "@joulev/theme": "workspace:*",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/node": "18.7.23",
    "@types/nprogress": "0.2.0",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "autoprefixer": "10.4.12",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "postcss": "8.4.17",
    "postcss-load-config": "4.0.1",
    "svelte": "3.50.1",
    "svelte-apollo": "0.5.0",
    "svelte-check": "2.9.1",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.1.8",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.8.4",
    "vite": "3.1.6"
  }
}
