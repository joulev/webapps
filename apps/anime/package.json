    "svelte-apollo": "0.5.0",
    "svelte-check": "3.1.2",
    "svelte-preprocess": "5.0.1",
    "tailwind-dark-aware": "0.2.1",
    "tailwindcss": "3.2.7",
    "ts-node": "10.9.1",
